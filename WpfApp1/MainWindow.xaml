<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        xmlns:corlib="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="600">
    <Grid>
        <Button Name="Press" Content="Нажми меня" HorizontalAlignment="Left" Margin="10,10,10,10" VerticalAlignment="Top" Width="180" Height="30" Click="Press_OnClick"></Button>
        <Calendar Name="MyCalendar" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,50,10,10"></Calendar>
    </Grid>
</Window>
